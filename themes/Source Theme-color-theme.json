                "fontStyle": "",
                "foreground": "#39c0ff"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "",
                "foreground": "#51FFC5"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "fontStyle": "",
                "foreground": "#51FFC5"
            }
        },
        {
            "name": "Tag attribute",
