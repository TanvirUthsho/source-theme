            "settings": {
                "foreground": "#ff3957",
                "fontStyle": ""
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#00d7e2"
            }
        },
        {
            "name": "diff.header",
