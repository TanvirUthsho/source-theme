                "foreground": "#ee774f",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Tag attribute",
            "scope": ["source"],
            "settings": {
                "foreground": "#ee774f"
            }
        },
        {
            "name": "Library function",
            "scope": "support.function",
            "settings": {
                "fontStyle": "",
                "foreground": "#ffd400"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
                "fontStyle": "",
                "foreground": "#39c0ff"
            }
        },
        {
            "name": "Library class/type",
