{
    "name": "Source Theme",
    "type": "dark",
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.quoted.docstring.multi.python"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#ffdde2b7"
                // "foreground": "#ff9271",
                // "foreground": "#FF8B39",
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#fede5d"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#ffd400"
            }
        },
        {
            "name": "Built-in constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#51FFC5" /**#51FFC5**/
            }
        },
        {
            "name": "User-defined constant",
            "scope": ["constant.character", "constant.other"],
            "settings": {
                "foreground": "#ff99e6"
            }
        },
        {
            "name": "Variable",
            "scope": "variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#9ffa55"
                // "foreground": "#ffd400"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#40a9ff"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "fontStyle": "",
                "foreground": "#e98484"
            }
        },
        {
            "name": "support.type",
            "scope": "support.type.vendored.property-name",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#f699cd"
            }
        },
        {
            "name": "Meta",
            "scope": "meta",
            "settings": {
                "foreground": "#51FFC5"
            }
        },
        {
            "name": "string",
            "scope": "string.quoted.double",
            "settings": {
                "foreground": "#FEDE5D"
            }
        },
        {
            "name": "string",
            "scope": "string.quoted.double.js",
            "settings": {
                "foreground": "#FF9CBE"
            }
        },
        {
            "name": "constant.language",
            "scope": "constant.language.boolean",
            "settings": {
                "foreground": "#ff9271",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Meta JSX",
            "scope": [
                "meta.jsx.children",
                "meta.jsx.children.js",
                "meta.jsx.children.tsx"
            ],
            "settings": {
                "foreground": "#f1ff62"
            }
        },
        {
            "name": "Variable Other Object JSX",
            "scope": [
                "variable.other.object.js",
                "variable.other.object.jsx",
                "variable.object.property.js",
                "variable.object.property.jsx"
            ],
            "settings": {
                "foreground": "#40a9ff"
            }
        },
        {
            "name": "Punctuation Parameters",
            "scope": "punctuation.definition.parameters",
            "settings": {
                "foreground": "#ffee80"
            }
        },
        {
            "name": "Punctuation Template Expression",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#ffee80"
            }
        },
        {
            "name": "Class name",
            "scope": ["entity.name.class", "entity.name.type.class"],
            "settings": {
                "fontStyle": "",
                "foreground": "#51FFC5"
            }
        },
        {
            "name": "Entity",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#fede5d"
            }
        },
        {
            "name": "keyword",
            "scope": ["keyword.control.import.include.php"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9EFFFF"
            }
        },
        {
            "name": "keyword",
