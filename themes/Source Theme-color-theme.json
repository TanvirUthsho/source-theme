            "settings": {
                "foreground": "#ec107b"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#dbcd00"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#029fcf"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#e92778"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#ffdf52"
                // "foreground": "#51FFC5"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                //"foreground": "#983dff"
                "foreground": "#a83dff"
            }
        },
